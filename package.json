  "private": true,
  "scripts": {
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android --port=3009",
    "ios": "react-native run-ios -- --reset-cache --clearCache --port=3008",
    "ios1": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/signature/main.jsbundle",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "build:ios1": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "build:ios2": "react-native bundle --minify --entry-file index.js --platform ios --dev false --bundle-output ./ios/main.jsbundle --assets-dest ./ios",
    "windows": "react-native run-windows",
    "link": "react-native link react-native-fs"
  },
